{
  "name": "solid-start",
  "version": "0.3.0-alpha.7",
  "description": "Create Solid apps with low configuration",
  "license": "MIT",
  "author": "Ryan Carniato",
  "homepage": "https://start.solidjs.com/",
  "bugs": "https://github.com/solidjs/solid-start/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/solidjs/solid-start.git",
    "directory": "packages/start"
  },
  "type": "module",
  "exports": {
    ".": "./index.tsx",
    "./router": "./router.tsx",
    "./vite": {
      "types": "./vite/plugin.d.ts",
      "import": "./vite/plugin.js"
    },
    "./addons/*": "./addons/*",
    "./api": "./api/index.ts",
    "./api/*": "./api/*",
    "./astro": {
      "types": "./astro/index.d.ts",
      "import": "./astro/index.js"
    },
    "./astro/*": "./astro/*",
    "./entry-client": "./entry-client/index.tsx",
    "./entry-client/*": "./entry-client/*",
    "./entry-server": "./entry-server/index.ts",
    "./entry-server/*": "./entry-server/*",
    "./error-boundary": "./error-boundary/index.tsx",
    "./error-boundary/*": "./error-boundary/*",
    "./islands": "./islands/index.tsx",
    "./islands/*": "./islands/*",
    "./root": "./root/index.tsx",
    "./root/*": "./root/*",
    "./data": "./data/index.ts",
    "./data/*": "./data/*",
    "./node/*": "./node/*",
    "./fs-router/*": "./fs-router/*",
    "./dev/*": "./dev/*",
    "./server": {
      "browser": "./server/browser.ts",
      "node": "./server/server.ts",
      "default": "./server/server.ts"
    },
    "./server/*": "./server/*",
    "./session": "./session/index.ts",
    "./session/*": "./session/*",
    "./websocket": "./websocket/index.ts",
    "./websocket/*": "./websocket/*",
    "./env": "./env.d.ts",
    "./package.json": "./package.json"
  },
  "main": "./index.tsx",
  "types": "./index.tsx",
  "bin": {
    "solid": "./bin.cjs",
    "solid-start": "./bin.cjs",
    "start": "./bin.cjs"
  },
  "files": [
    "api",
    "astro",
    "data",
    "dev",
    "entry-client",
    "entry-server",
    "error-boundary",
    "fs-router",
    "islands",
    "node",
    "root",
    "server",
    "session",
    "virtual",
    "vite",
    "websocket",
    "bin.cjs",
    "index.tsx",
    "router.tsx",
    "required-user-files.d.ts",
    "env.d.ts",
    "types.ts"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit --checkJs false --skipLibCheck"
  },
  "dependencies": {
    "@astrojs/solid-js": "^2.1.0",
    "@babel/core": "^7.20.12",
    "@babel/generator": "^7.20.7",
    "@babel/plugin-syntax-jsx": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/template": "^7.20.7",
    "@types/cookie": "^0.5.1",
    "babel-preset-solid": "^1.7.2",
    "chokidar": "^3.5.3",
    "compression": "^1.7.4",
    "connect": "^3.7.0",
    "debug": "^4.3.4",
    "dequal": "^2.0.3",
    "es-module-lexer": "^1.1.0",
    "esbuild": "^0.17.15",
    "esbuild-plugin-solid": "^0.5.0",
    "fast-glob": "^3.2.12",
    "get-port": "^6.1.2",
    "parse-multipart-data": "^1.5.0",
    "picocolors": "^1.0.0",
    "sade": "^1.8.1",
    "set-cookie-parser": "^2.5.1",
    "terser": "^5.16.1",
    "vite-plugin-inspect": "^0.7.14",
    "vite-plugin-solid": "^2.7.0",
    "vitefu": "0.2.4",
    "solid-ssr": "1.7.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.19.0",
    "@solidjs/meta": "^0.28.2",
    "@solidjs/router": "^0.8.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/babel__core": "^7.20.0",
    "@types/debug": "^4.1.7",
    "@types/node": "^18.11.18",
    "@types/wait-on": "^5.3.1",
    "astro": "^2.5.5",
    "jsdom": "^20.0.3",
    "solid-js": "^1.7.0",
    "solid-testing-library": "^0.3.0",
    "typescript": "^4.9.4",
    "vite": "^4.3.9",
    "vitest": "^0.20.3"
  },
  "peerDependencies": {
    "@solidjs/meta": "^0.28.0",
    "@solidjs/router": "^0.8.2",
    "astro": "^2.5.5",
    "solid-js": "^1.7.0"
  }
}
